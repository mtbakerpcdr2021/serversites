          "_customHTML": "<section class=\"menu horizontal-menu\" group=\"Menu\" plugins=\"DropDown, amp-main,amp-sidebar\" global once=\"menu\">\n\n    <mbr-parameters>\n        <!-- Block parameters controls (Blue \"Gear\" panel) -->\n        <header>Show/Hide</header>\n        <input type=\"checkbox\" title=\"Show Logo\" name=\"showLogo\">\n        <input type=\"checkbox\" title=\"Show Contacts\" name=\"showContacts\" checked>\n        <input type=\"range\" inline title=\"Icons Count\" name=\"iconsCount\" min=\"1\" max=\"5\" value=\"1\" step=\"1\">\n        <input type=\"checkbox\" title=\"Show Brand Name\" name=\"showBrand\">\n        <input type=\"checkbox\" title=\"Show Button(s)\" name=\"showButtons\">\n        <input type=\"checkbox\" title=\"Sticky\" name=\"sticky\" checked>\n        <select title=\"Sidebar Side\" name=\"sidebarSide\">\n            <option value=\"left\">Left</option>\n            <option value=\"right\" selected>Right</option>\n        </select>\n        <header>Colors</header>\n        <input type=\"color\" title=\"Title BG Color\" name=\"menuBgColor3\" value=\"#ffffff\">\n        <input type=\"color\" name=\"icons\" title=\"Icons Color\" value=\"#ffffff\">\n        <input type=\"color\" name=\"ddcolor\" title=\"Drop Down BG Color\" value=\"#ffffff\">\n        <input type=\"color\" name=\"ddcolorlink\" title=\"Drop Down Link Color\" value=\"#420676\">\n        <!-- End block parameters -->\n    </mbr-parameters>\n    <nav class=\"navbar navbar-dropdown navbar-expand-lg\" mbr-class=\"{'navbar-fixed-top':sticky}\">\n        <div class=\"top-menu\">\n            <div class=\"top-menu-wrapper\">\n                <div class=\"top-box\">\n\n                    <div class=\"brand\" mbr-if=\"showLogo || showBrand\">\n                        <span mbr-if=\"showLogo\" class=\"brand-logo\">\n                            <amp-img src=\"@PROJECT_PATH@/assets/images/computeronly-120x115.png\" layout=\"responsive\" width=\"62.608695652173914\" height=\"60\" alt=\"image\" class=\"mobirise-loader\">\n                                <div placeholder class=\"placeholder\"></div>\n                                <a href=\"index.html\"><img class=\"mbr-temp\" src=\"@PROJECT_PATH@/assets/images/computeronly-120x115.png\" alt data-aspectratio=\"1\"></a>\n                            </amp-img>\n                        </span>\n                        <p mbr-text mbr-theme-style=\"display-5\" mbr-if=\"showBrand\" class=\"brand-name mbr-fonts-style mbr-bold\" data-app-selector=\".brand-name\">Webb&nbsp;<br>Computer<br>repair</p>\n                    </div>\n                </div>\n                <div class=\"top-box\">\n\n                    <p mbr-text mbr-theme-style=\"display-4\" class=\"phone-text hide-top mbr-fonts-style mbr-regular\" data-app-selector=\".phone-text\">Webb Computer Repair-Mcalester Ok</p>\n                    <div class=\"item-wrapper hide-top\" mbr-if=\"showContacts\">\n                        <span class=\"iconfont-wrapper\">\n\n                            <span mbr-icon class=\"amp-iconfont contact-icon mobi-mbri-letter mobi-mbri\"></span>\n                        </span>\n                        <p mbr-text mbr-theme-style=\"display-4\" class=\"brand-text mbr-fonts-style mbr-regular\" data-app-selector=\".brand-text, .brand\"><a href=\"mailto:webbcomputerrepair@protonmail.com\" class=\"text-white\">webbcomputerrepair@protonmail.com</a></p>\n                    </div>\n                    <div class=\"item-wrapper  hide-top\" mbr-if=\"showContacts\">\n                        <span class=\"iconfont-wrapper\">\n                            <span mbr-icon class=\"amp-iconfont contact-icon fa-phone fa\"></span>\n                        </span>\n                        <p mbr-text mbr-theme-style=\"display-4\" class=\"brand-text mbr-fonts-style mbr-regular\" data-app-selector=\".brand-text, .brand\">(918)558-9544</p>\n\n                    </div>\n                </div>\n            </div>\n        </div>\n        <div class=\"collapse navbar-collapse\" id=\"navbarSupportedContent\">\n            <div>\n                <button class=\"navbar-toggler mb-2\" type=\"button\" data-toggle=\"collapse\" data-target=\"#navbarSupportedContent\" aria-controls=\"navbarNavAltMarkup\" aria-expanded=\"false\" aria-label=\"Toggle navigation\">\n                    <div class=\"hamburger\">\n                        <span></span>\n                        <span></span>\n                        <span></span>\n                        <span></span>\n                    </div>\n                </button>\n                <ul mbr-menu class=\"navbar-nav nav-dropdown\" mbr-theme-style=\"display-4\" mbr-class=\"{'nav-right': !showButtons,'navbar-nav-top-padding': isPublish && !showBrand && !showLogo}\"><li class=\"nav-item\"><a class=\"nav-link link text-white text-primary\" href=\"tel:+1-918-558-9544\" data-app-selector=\".nav-link,.dropdown-item\" data-app-placeholder=\"Type Text\">CALL US NOW</a></li></ul>\n            </div>\n            <div class=\"menu-box\">\n\n                <div class=\"icons-list\" mbr-list mbr-list-grow=\"iconsCount\">\n                    <a href=\"https://www.facebook.com/WebbComputerRepairhttps://www.facebook.com/pages/Mobirise/1616226671953247\" target=\"_blank\">\n                        <span class=\"iconfont-wrapper\">\n                            <span mbr-icon class=\"amp-iconfont fa-facebook-square fa\"></span>\n                        </span>\n                    </a>\n                </div>\n                <div mbr-if=\"showButtons\" mbr-buttons mbr-theme-style=\"display-7\" class=\"navbar-buttons mbr-section-btn\"><a class=\"btn btn-sm btn-primary\" href=\"https://mobiri.se\" data-app-placeholder=\"Type Text\">\n                        Contact Us\n                    </a></div>\n            </div>\n       </div>\n        <button class=\"navbar-toggler hide\" type=\"button\" data-toggle=\"collapse\" data-target=\"#navbarSupportedContent\" aria-contro